{
    "manifest_version": 3,
    "version":"1.0",
    "name":"Popup Extension",
    "description":"Show Titles",
    
    "permissions":["activeTab", "scripting", "tabs"],
    "action":{
        "default-tittle":"Show title of webpage",
        "default_popup":"index.html",
        "default_icon":{
            "24":"./Icons/icon1.png",
            "32":"./Icons/icon1.png",
            "48":"./Icons/icon1.png"
        }
    }
}